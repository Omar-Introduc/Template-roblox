{
  "name": "Template-roblox",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "Events": {
        "$className": "Folder",
        "PointAwarded": {
          "$className": "RemoteEvent"
        },
        "BuyItem": {
          "$className": "RemoteFunction"
        }
      }
    },
    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },
    "ServerStorage": {
      "Prefabs": {
        "$path": "src/assets"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    },
    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    }
  }
}